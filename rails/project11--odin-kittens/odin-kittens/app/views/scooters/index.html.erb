<p id="notice"><%= notice %></p>

<h1>Listing Scooters</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scooters.each do |scooter| %>
      <tr>
        <td><%= link_to 'Show', scooter %></td>
        <td><%= link_to 'Edit', edit_scooter_path(scooter) %></td>
        <td><%= link_to 'Destroy', scooter, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Scooter', new_scooter_path %>
